{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "plugins": ["plugins/markdown","plugins/summarize","jsdoc-mermaid"],
    "templates": {
        "systemName": "Becca Server",
        "logoFile": "",
        "theme": "flatly",
        "cleverLinks": true,
        "monospaceLinks": false,
        "dateFormat": "ddd MMM Do YYYY",
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "footer": "",
        "copyright": "Copyright Â© 2018-2019 - Eric Moore, & the contributors to Becca projects.",
        "navType": "vertical",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true,
        "protocol": "html://",
        "methodHeadingReturns": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}



    
    
