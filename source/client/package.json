{
  "name": "becca-client",
  "version": "1.0.0",
  "description": "## User Stories",
  "main": "src/index.js",
  "scripts": {
    "_start": "webpack-dev-server --hot --open --inline --color",
    "_startHTTPS": "webpack-dev-server --hot --open --https --inline --color --cert=../../../../.localhost-ssl/cert.pem --key=../../../../.localhost-ssl/key.pem",
    "_startHTTPSOtherCerts": "webpack-dev-server --hot --open --https --inline --color --cert=node_modules/webpack-dev-server/ssl/server.pem --key=node_modules/webpack-dev-server/ssl/server.pem",
    "start": "npm run _start -- --env.mode development",
    "start:HTTPS": "npm run _startHTTPS -- --env.mode development",
    "start:otherCerts": "npm run _startHTTPSOtherCerts -- --env.mode development",
    "start:withSources": "npm run start -- --env.presets sourceMaps",
    "start:dev": "npm run _start -- --env.mode development",
    "start:prod": "npm run _start -- --env.mode production",
    "devServer:debug": "node --inspect --inspect-brk ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start:debug:dev": "npm run devServer:debug -- --env.mode development --env.presets sourceMaps --env.sourcemap cheap-module-eval-source-map",
    "start:debug:prod": "npm run devServer:debug -- --env.mode production",
    "_webpack": "webpack",
    "webpack": "npm run _webpack -- --env.mode development",
    "webpack:dev:watch": "npm run _webpack -- --env.mode development --watch",
    "webpack:dev:stats": "npm run _webpack -- --env.mode development --profile --json > stats.json",
    "dev": "rm -rf ./dist/ && npm run _webpack -- --env.mode development",
    "prod": "rm -rf ./dist/ && npm run _webpack -- --env.mode production",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev:debug": "npm run debug -- --env.mode development",
    "prod:debug": "npm run debug -- --env.mode production",
    "dev:display": "npm run prod -- --display-modules",
    "prod:display": "npm run prod -- --display-modules",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "test": "nyc -r lcov npx tead && nyc report",
    "test:watch": "npx tead --watch",
    "docs": "rm -rf docs/ && jsdoc ./src -r -c jsdocs.conf.json -d docs -t ./node_modules/ink-docstrap/template -R README.md && open docs/index.html",
    "doc:test": "rm -rf out/  && jsdoc src/actions/auth.js -c jsdocs.conf.json && open out/index.html",
    "doc:template": "jsdoc src/actions/auth.js -c jsdocs.conf.json -t ./node_modules/ink-docstrap/template -R README.md && open out/index.html",
    "@help:docs": "open http://usejsdoc.org/about-commandline.html"
  },
  "author": "Eric Moore",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@hyperapp/router": "^0.7.1",
    "classcat": "^3.2.3",
    "date-fns": "^2.0.0-alpha.27",
    "hyperapp": "^1.2.9",
    "lodash-es": "^4.17.11",
    "ramda": "^0.26.1",
    "spectre.css": "^0.5.8"
  },
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
