    "srvs": "^0.3.2",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "tead": "^0.5.1",
    "terser-webpack-plugin": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
